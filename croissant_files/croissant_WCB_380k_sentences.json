        },
        {
          "@type": "cr:Field",
          "@id": "default/sentence",
          "dataType": "sc:Text",
          "source": {
            "fileSet": { "@id": "parquet-files-for-config-default" },
            "extract": { "column": "sentence" }
          }
        }
      ]
    }
